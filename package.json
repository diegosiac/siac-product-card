{
  "author": "DiegoADC",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "7.15.6",
    "@babel/preset-react": "7.14.5",
    "@rollup/plugin-image": "2.1.1",
    "@size-limit/preset-small-lib": "5.0.5",
    "@types/react": "17.0.26",
    "@types/react-dom": "17.0.9",
    "@types/react-test-renderer": "17.0.1",
    "babel-jest": "27.2.4",
    "husky": "7.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.2.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "rollup-plugin-postcss": "4.0.1",
    "size-limit": "5.0.5",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://diegosiac.com",
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    }
  },
  "keywords": [
    "product",
    "card",
    "diego",
    "react",
    "components"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/siac-product-card.esm.js",
  "name": "siac-product-card",
  "peerDependencies": {
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diegosiac/siac-product-card"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --watch"
  },
  "size-limit": [
    {
      "limit": "10 KB",
      "path": "dist/siac-product-card.cjs.production.min.js"
    },
    {
      "limit": "10 KB",
      "path": "dist/siac-product-card.esm.js"
    }
  ],
  "typings": "dist/index.d.ts",
  "version": "0.0.1"
}